  <process id="WhiteListProcess" name="WhiteListProcess" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="whiteListApplicant" name="白名单提交申请" activiti:assignee="${userId}"></userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="whiteListApplicant"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow2" sourceRef="whiteListApplicant" targetRef="exclusivegateway1"></sequenceFlow>
    <userTask id="branchProcess" name="分公司子流程" activiti:assignee="${UserRole.userId}" activiti:candidateGroups="${UserRole.roleCode}">
      <extensionElements>
        <activiti:taskListener event="create" class="com.cpic.ruleengine.custmanage.service.whitelistlistener.BranchProcessCreateImpl"></activiti:taskListener>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="${branchRoleList}" activiti:elementVariable="UserRole">
        <completionCondition>${branchPassCount/branchTotalCount == 1|| branchNoPassCount &gt;0}</completionCondition>
      </multiInstanceLoopCharacteristics>
    </userTask>
    <sequenceFlow id="flow3" name="分公司" sourceRef="exclusivegateway1" targetRef="branchProcess">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${branchRoute == '1'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="headquarters" name="总公司子流程" activiti:assignee="${headquartersRole.userId}" activiti:candidateGroups="${headquartersRole.roleCode}">
      <extensionElements>
        <activiti:taskListener event="create" class="com.cpic.ruleengine.custmanage.service.whitelistlistener.HeadquartersImpl"></activiti:taskListener>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="${headquartersRoleList}" activiti:elementVariable="headquartersRole">
        <completionCondition>${headquartersPassCount/headquartersTotalCount ==1||headquartersNoPassCount &gt;0}</completionCondition>
      </multiInstanceLoopCharacteristics>
    </userTask>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow5" sourceRef="branchProcess" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow6" name="通过" sourceRef="exclusivegateway2" targetRef="headquarters">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${branchPassCount/branchTotalCount == 1}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow7" name="不通过" sourceRef="exclusivegateway2" targetRef="whiteListApplicant">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${branchNoPassCount >0}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow8" sourceRef="headquarters" targetRef="exclusivegateway3"></sequenceFlow>
    <userTask id="finalAuditorOfHeadOffice" name="总公司名单终审" activiti:assignee="${headOfficeFinalUserId}" activiti:candidateGroups="headOfficeFinalAuditor"></userTask>
    <exclusiveGateway id="exclusivegateway5" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow15" name="不通过" sourceRef="exclusivegateway5" targetRef="whiteListApplicant">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass == 'no'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow17" name="不通过" sourceRef="exclusivegateway3" targetRef="whiteListApplicant">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${headquartersNoPassCount >0}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow18" name="总公司核保员和分公司参数管理员" sourceRef="exclusivegateway1" targetRef="headquarters">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${branchRoute == '2'}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway6" name="终审"></exclusiveGateway>
    <sequenceFlow id="flow20" name="通过" sourceRef="exclusivegateway3" targetRef="exclusivegateway6">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${headquartersPassCount/headquartersTotalCount ==1}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow21" name="总公司参数管理员" sourceRef="exclusivegateway1" targetRef="exclusivegateway6">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${branchRoute=='3'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow22" name="需要" sourceRef="exclusivegateway6" targetRef="finalAuditorOfHeadOffice">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isNeed =='yes'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow25" sourceRef="finalAuditorOfHeadOffice" targetRef="exclusivegateway5"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow27" name="通过" sourceRef="exclusivegateway5" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass == 'yes'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow28" name="不需要" sourceRef="exclusivegateway6" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isNeed =='no'}]]></conditionExpression>
    </sequenceFlow>
  </process>
